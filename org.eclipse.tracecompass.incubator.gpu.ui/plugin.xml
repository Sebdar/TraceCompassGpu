<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="org.eclipse.tracecompass.incubator.gpu.ui.views.GpuRooflineView"
            id="org.eclipse.tracecompass.incubator.gpu.ui.roofline"
            name="GPU Roofline"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <command
               commandId="org.eclipse.tracecompass.incubator.gpu.ui.rooflineconfigure"
               label="Configure"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="org.eclipse.linuxtools.tmf.ui.commands.category"
            description="%command.description"
            id="org.eclipse.tracecompass.incubator.gpu.ui.gpurooflineconfigure"
            name="%command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="org.eclipse.tracecompass.incubator.gpu.ui.gpurooflinehandler">
      </handler>
   </extension>

</plugin>
